{
	"compilerOptions": {
		"jsx": "react",
		"strict": true,
		"strictNullChecks": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"target": "es5",
		"module": "commonjs",
		"allowSyntheticDefaultImports": true,
		"lib": ["es2019", "DOM"],
		"moduleResolution": "node",
		"esModuleInterop": true,
		"experimentalDecorators": true
	}
}
