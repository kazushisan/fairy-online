{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended",
		"tslint-config-prettier"
	],
	"rules": {
		"variable-name": [true, "ban-keywords", "check-format", "allow-snake-case", "allow-pascal-case"],
		"class-name": false,
		"interface-name": [true, "never-prefix"],
		"no-console": [false],
		"prettier": true,
		"object-literal-sort-keys": false
	},
	"rulesDirectory": [
		"tslint-plugin-prettier"
	]
}